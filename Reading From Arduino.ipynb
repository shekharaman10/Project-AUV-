   "source": [
    "import serial\n",
    "serialInst = serial.Serial()\n",
    "ports='COM4'\n",
    "serialInst.baudrate = 9600\n",
    "serialInst.port = ports\n",
    "serialInst.open()\n",
    "\n",
    "while True:\n",
    "\tif serialInst.in_waiting:\n",
    "\t\tdata = serialInst.readline()\n",
    "\t\tprint(data.decode('utf-8').rstrip('\\r\\n'))"
   ]
  },
